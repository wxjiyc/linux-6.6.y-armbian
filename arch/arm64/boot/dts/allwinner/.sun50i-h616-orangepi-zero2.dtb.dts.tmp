# 8 "arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/clock/sun50i-h6-r-ccu.h" 1
# 9 "arch/arm64/boot/dts/allwinner/sun50i-h616.dtsi" 2
